{
    "name": "artem328/laravel-yandex-money",
    "type": "library",
    "description": "Yandex Money integration with Laravel framework",
    "keywords": [
        "Artem328",
        "LaravelYandexMoney"
    ],
    "homepage": "https://github.com/artem328/laravel-yandex-money",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Vozhzhov",
            "email": "vojjov.artem@ya.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Artem328\\LaravelYandexMoney\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artem328\\LaravelYandexMoney\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
